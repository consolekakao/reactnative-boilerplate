{
  "name": "boilerPlate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "android:clean": "cd android && ./gradlew clean && cd ../",
    "ios:init": "cd ios; pod install; cd ..",
    "ios:clean:build": "cd ios && xcodebuild clean && cd ../",
    "ios:clean": "cd ios; pod deintegrate; pod cache clean --all; cd ..",
    "ios:reset": "yarn ios:clean; yarn ios:init",
    "start": "yarn watch:clean; react-native start --reset-cache",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "react": "18.1.0",
    "react-native": "0.70.6",
    "axios": "^1.2.1",
    "moment": "^2.29.4",
    "@gorhom/bottom-sheet": "^4",
    "reanimated-bottom-sheet": "^1.0.0-alpha.22",
    "styled-components": "^5.3.6",
    "@react-native-async-storage/async-storage": "^1.17.11",
    "@react-native-community/datetimepicker": "^6.7.1",
    "@react-navigation/native": "^6.1.1",
    "@react-navigation/native-stack": "^6.9.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/react": "^18.0.21",
    "@types/react-native": "^0.70.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "0.72.3",
    "react-test-renderer": "18.1.0",
    "typescript": "^4.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
